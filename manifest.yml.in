rp-manifest: 1

id: @NAME@
version: @PROJECT_VERSION@
name: @NAME@
author: IoT.bzh
license: GPL

description:
  This is the tux-evse demo.

required-api:
  - name: helloworld
    value: auto
  - name: i2c
    value: auto
  - name: tiam62x
    value: auto
  - name: tux-evse-webapp-mock
    value: auto
    
targets:
  - target: main
    content:
      src: bin/start_hub_bender.sh
      type: application/x-executable

file-properties:
  - name: bin/start_hub_bender.sh
    value: executable
  - name: htdocs/favicon.ico
    value: public
  - name: htdocs/index.html
    value: public
  - name: htdocs/main.js
    value: public
  - name: htdocs/polyfills.js
    value: public
  - name: htdocs/runtime.js
    value: public
  - name: htdocs/styles.css
    value: public
    
required-permission:
#  - urn:AGL:permission::system:run-by-default
  - acl:lock:motor
  - acl:ic2:0
